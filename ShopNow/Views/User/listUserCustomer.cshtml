@model ShopNow.ViewModels1.UserCustomerModel

@{
    ViewBag.Title = "listUserCustomer";
 }

<h2>listUserCustomer</h2>
<div>UserCount: @Model.UserCount  </div>
<div>
<table class="table table-dark table-bordered table-hover table-sm" border="1">
    <tr>
        <th>
            User Id
        </th>
        <th>
            User Name
        </th>
        <th>
            Email Id
        </th>
        <th>
            Mobile No
        </th>
        <th>
            Role Id
        </th>
        <th>
            Password
        </th>
        <th>
            City
        </th>
        <th>
            Store Id
        </th>
        <th>
            Created By
        </th>
        <th>
            Created Date
        </th>


    </tr>

    @foreach (var item in Model.listUser)
    {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.UserName
            </td>
            <td>
                @item.EmailId
            </td>
            <td>
                @item.MobileNo
            </td>
            <td>
                @item.RoleId
            </td>
            <td>
                @item.Password
            </td>
            <td>
                @item.City
            </td>
            <td>
                @item.StoreId
            </td>
            <td>
                @item.CreatedBy
            </td>
            <td>
                @item.CreatedDate
            </td>
        </tr>
    }
      </table>
</div>
    
<div>Customer Count:  @Model.CustomerCount</div>
<div>
    <table class="table table-dark table-bordered table-hover table-sm"
           border="1">
        <tr>
            <th>
                Cust Id
            </th>
            <th>
                Customer Name
            </th>
            <th>
                Mobile No
            </th>
            <th>
                Created By
            </th>
            <th>
                Created Date
            </th>
            


        </tr>

        @foreach (var item in Model.listCustomer)
        {
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.CustomerName
                </td>
                <td>
                    @item.MobileNo
                </td>
                <td>
                    @item.CreatedBy
                </td>
                <td>
                    @item.CreatedDate
                </td>
                
            </tr>
        }
    </table>
</div>

